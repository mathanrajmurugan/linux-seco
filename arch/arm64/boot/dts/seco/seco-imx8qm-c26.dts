				/* Input Signals */
				SC_P_ESAI0_FSR_LSIO_GPIO2_IO22		0x00000021 /* otg - USB_VBUS_3V3 */	
				SC_P_ESAI1_FST_LSIO_GPIO2_IO05		0x00000021 /* THRMTRIP# */

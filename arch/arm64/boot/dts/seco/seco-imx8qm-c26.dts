				SC_P_ESAI0_TX0_LSIO_GPIO2_IO26		0x00000021 /* HDMI IN - HDMI_IN_MUX_SEL2 */
				SC_P_ESAI0_TX1_LSIO_GPIO2_IO27		0x00000021 /* HDMI IN - HDMI_IN_MUX_EN */	

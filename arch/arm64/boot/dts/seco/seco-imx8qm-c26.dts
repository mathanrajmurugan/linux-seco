				/* PCIE CLK OE 1 */
				SC_P_ENET1_RGMII_TXD0_LSIO_GPIO6_IO12	0x00000021
				/* PCIE CLK OE 0 */
				SC_P_ENET1_RGMII_TXD1_LSIO_GPIO6_IO13	0x00000021
